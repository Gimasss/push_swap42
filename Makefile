NAME 		= push_swap
CC 			= cc
CFLAGS 		= -g -Wall -Wextra -Werror 
RM 			= rm -f
INC 		= -I./libft/includes -I.
LIBFTPATH 	= ./libft
SRC 		= push_swap.c list_utils.c list_basic.c atoi_evo.c \
			cleaner.c rotating.c rev_rotating.c pushing.c swapping.c \
			sorting.c planning.c algo.c

all: $(NAME)

$(NAME): $(SRC)
	$(MAKE) -C $(LIBFTPATH)
	$(CC) $(CFLAGS) $(INC) $(SRC) -Llibft -lft -o $(NAME)
	@$(MAKE) text
	@$(MAKE) luna

        
clean:
	$(MAKE) clean -C $(LIBFTPATH)
	@$(MAKE) txtcln

fclean: clean
	$(MAKE) fclean -C $(LIBFTPATH)
	
	$(RM) $(NAME)

re: fclean all

.PHONY: all clean fclean re
.SILENT: 

text:
	@echo -e '\033[1;95müåôIn the name of the moon, compile all‚ú®\033[0m'

txtcln:
	@echo -e '\033[1;94mPartially cleanedüßº \033[0m'


